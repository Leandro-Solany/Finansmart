{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DatabaseConnection": "#{DatabaseConnection}#"
  },
  "Swagger": {
    "Enabled": true,
    "Title": "Finansmart API",
    "Version": "v1",
    "Description": "API para gerenciamento financeiro e cursos"
  },
  "Azure": {
    "ApplicationInsights": {
      "InstrumentationKey": "#{ApplicationInsights:InstrumentationKey}#"
    }
  }
}
